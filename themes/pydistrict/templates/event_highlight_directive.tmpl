## -*- coding: utf-8 -*-
<%block name="content">
<!-- Begin post-list ${post_list_id} -->
<div id="${post_list_id}">
    % for post in posts:
        <div class="event">

            <h1>${post.title(lang)|h}</h1>
            ${post.text(teaser_only=True)}
        </div>
    % endfor
</div>
<!-- End post-list ${post_list_id} -->
</%block>
